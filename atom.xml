---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>yagri.com - You Ain't Gonna Read It</title>
 <link rel="self" href="http://yagri.com/atom.xml"/>
 <link href="http://yagri.com/"/>
 <icon>http://yagri.com{{ site.root }}/favicon.ico</icon>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://yagri.com/</id>
 <author>
   <name>Jon Merrifield</name>
 </author>
 
 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <id>http://yagri.com{{ site.root }}/{{ post.url }}</id>
   <link href="http://yagri.com{{ site.root }}/{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <published>{{ post.date | date_to_xmlschema }}</published>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
 
</feed>